{
    "name": "ithemes/ithemes-exchange",
    "description": "Easily sell your digital goods with iThemes Exchange, simple ecommerce for WordPress",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "10up/wp_mock": "dev-master"
    },
    "license": "GPLv2",
    "authors": [
        {
            "name": "iThemes",
            "email": "sales@ithemes.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "asika/autolink": "dev-master",
        "ironbound/db": "^2.0.0",
        "ironbound/wpevents": "^1.0",
        "justinrainbow/json-schema": "5.1",
        "firebase/php-jwt": "^4.0",
        "ironbound/db-logger": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TimothyBJacobs/php-autolink"
        }
    ],
    "config": {
      "platform": {
            "php": "5.3.29"
        }
    },
    "autoload": {
        "psr-4": {
            "iThemes\\Exchange\\REST\\": "lib/REST/",
            "iThemes\\Exchange\\CLI\\": "lib/cli/",
            "ITETOS\\": "core-addons/admin/terms-of-service/"
        }
    }
}
