{
    "name": "ithemes/ithemes-exchange",
    "description": "Easily sell your digital goods with iThemes Exchange, simple ecommerce for WordPress",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "10up/wp_mock": "^0.1.1"
    },
    "license": "GPLv2",
    "authors": [
        {
            "name": "iThemes",
            "email": "sales@ithemes.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "asika/autolink": "dev-master",
        "ironbound/db": "dev-master",
        "ironbound/wpevents": "^1.0",
        "justinrainbow/json-schema": "^4.0",
        "firebase/php-jwt": "^4.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TimothyBJacobs/php-autolink"
        }
    ],
    "config": {
      "platform": {
            "php": "5.3.29"
        }
    },
    "autoload": {
        "psr-4": {"iThemes\\Exchange\\REST\\": "lib/REST/"}
    }
}
